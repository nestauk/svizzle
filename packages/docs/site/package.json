{
	"description": "Svizzle documentation site",
	"dependencies": {
		"@svizzle/atlas": "^0.8.0",
		"@svizzle/barchart": "^0.8.1",
		"@svizzle/choropleth": "^0.9.1",
		"@svizzle/geo": "^0.9.0",
		"@svizzle/histogram": "^0.6.1",
		"@svizzle/legend": "^0.4.1",
		"@svizzle/time_region_value": "^0.8.1",
		"@svizzle/ui": "^0.8.0",
		"@svizzle/utils": "^0.18.0",
		"d3-color": "^3.1.0",
		"d3-geo": "^3.0.1",
		"d3-scale": "^4.0.2",
		"lamb": "^0.61.0",
		"svelte-json-tree": "^1.0.0"
	},
	"devDependencies": {
		"@babel/core": "^7.19.0",
		"@babel/plugin-syntax-dynamic-import": "^7.8.3",
		"@babel/plugin-transform-runtime": "^7.18.10",
		"@babel/preset-env": "^7.19.0",
		"@babel/runtime": "^7.19.0",
		"@rollup/plugin-commonjs": "^22.0.2",
		"@rollup/plugin-json": "^4.1.0",
		"@rollup/plugin-node-resolve": "^14.0.1",
		"@rollup/plugin-replace": "^4.0.0",
		"@svizzle/dev": "^0.6.0",
		"@svizzle/file": "^0.14.0",
		"chance": "^1.1.8",
		"compression": "^1.7.4",
		"d3-array": "^3.2.0",
		"d3-dsv": "^3.0.1",
		"eslint": "^8.23.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"npm-run-all": "^4.1.5",
		"polka": "next",
		"rimraf": "^3.0.2",
		"rollup": "^2.79.0",
		"rollup-plugin-alias": "^2.2.0",
		"rollup-plugin-babel": "^4.4.0",
		"rollup-plugin-svelte": "^7.1.0",
		"rollup-plugin-terser": "^7.0.2",
		"sapper": "^0.29.3",
		"sirv": "^2.0.2",
		"svelte": "^3.50.1"
	},
	"engines": {
		"node": ">=17.5.0"
	},
	"name": "@svizzle/site",
	"private": true,
	"scripts": {
		"sitebuild": "sapper build --legacy",
		"dev": "npm run rmsappers && sapper dev",
		"chmod": "chmod u+x src/bin/*",
		"copyAtlas": "rimraf ./static/atlas && cp -R ./node_modules/@svizzle/atlas/data/dist ./static/atlas",
		"export": "npm run chmod && node src/bin/makeEntries.js | xargs sapper export --legacy --basepath svizzle --concurrent 1 --entry",
		"lint": "eslint --max-warnings=0 rollup.config.js 'src/**/*.{js,svelte}'",
		"makeEntries": "npm run chmod && node src/bin/makeEntries.js",
		"makeRandomBins": "npm run chmod && node src/bin/makeRandomBins.js",
		"makeTRVdata": "npm run chmod && rimraf static/data/*.csv && node src/bin/makeRandomIndicators.js",
		"outdated": "npm outdated",
		"postinstall": "npm run copyAtlas",
		"rmsappers": "rimraf __sapper__ src/node_modules/@sapper",
		"start": "node __sapper__/build",
		"TODO_test": "run-p --race dev cy:run"
	},
	"type": "module",
	"version": "0.4.1"
}
