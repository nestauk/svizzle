{
	"author": "nestauk (https://www.nesta.org.uk/)",
	"bugs": {
		"url": "https://github.com/nestauk/svizzle/issues"
	},
	"devDependencies": {
		"eslint": "^6.8.0",
		"eslint-plugin-import": "^2.18.2",
		"esm": "^3.0.84",
		"lamb": "^0.58.0",
		"mkdirp": "^0.5.1",
		"mocha": "^5.2.0",
		"rimraf": "^3.0.0",
		"rollup": "^1.20.0",
		"rollup-plugin-analyzer": "^3.1.2",
		"rollup-plugin-buble": "^0.19.8",
		"rollup-plugin-cleanup": "^3.1.1",
		"rollup-plugin-commonjs": "^10.0.2",
		"rollup-plugin-node-resolve": "^5.2.0",
		"rollup-plugin-terser": "^5.2.0"
	},
	"description": "Svizzle Dev contains development helpers.",
	"files": [
		"dist/*",
		"index.js",
		"LICENSE",
		"src/**/*.js",
		"!pkg/*",
		"!*.spec.js"
	],
	"homepage": "https://github.com/nestauk/svizzle",
	"keywords": [
		"development",
		"functional programming",
		"utility"
	],
	"license": "MIT",
	"main": "dist/index.js",
	"module": "index.js",
	"name": "@svizzle/dev",
	"peerDependencies": {
		"lamb": "^0.58.0"
	},
	"publishConfig": {
		"access": "public"
	},
	"repository": {
		"type": "git",
		"url": "github:nestauk/svizzle",
		"directory": "packages/tools/dev"
	},
	"scripts": {
		"build": "rollup -c",
		"bumppack": "npm run setprerelease && npm run pack",
		"lint": "eslint rollup.config.js src/*.js",
		"pack": "mkdirp pkg && rimraf pkg/* && dirname=${PWD##*/} && tar -cvz -f pkg/${npm_package_version}.tar.gz -C .. ${dirname}/dist/ ${dirname}/index.js ${dirname}/LICENSE ${dirname}/package.json ${dirname}/src/",
		"setpreminor": "npm --no-git-tag-version version preminor --preid=dev",
		"setprerelease": "npm --no-git-tag-version version prerelease --preid=dev",
		"test": "mocha -r esm --recursive src"
	},
	"sideEffects": true,
	"version": "0.3.0"
}
