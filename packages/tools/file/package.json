{
    "author": "nestauk (https://www.nesta.org.uk/)",
    "bugs": {
        "url": "https://github.com/nestauk/svizzle/issues"
    },
    "dependencies": {
        "@svizzle/utils": "^0.7.1",
        "d3-dsv": "^1.2.0"
    },
    "description": "Svizzle File contains file related helpers for node.",
    "devDependencies": {
        "@svizzle/dev": "^0.3.0",
        "@svizzle/request": "^0.2.0",
        "eslint": "^6.8.0",
        "eslint-plugin-import": "^2.18.2",
        "esm": "^3.0.84",
        "lamb": "^0.58.0",
        "mkdirp": "^0.5.1",
        "mocha": "^5.2.0",
        "nock": "^10.0.6",
        "node-fetch": "^2.6.0",
        "rimraf": "^3.0.0",
        "rollup": "^1.20.0",
        "rollup-plugin-analyzer": "^3.1.2",
        "rollup-plugin-buble": "^0.19.8",
        "rollup-plugin-cleanup": "^3.1.1",
        "rollup-plugin-commonjs": "^10.0.2",
        "rollup-plugin-node-resolve": "^5.2.0",
        "rollup-plugin-terser": "^5.2.0",
        "tempy": "^0.2.1"
    },
    "files": [
        "dist/*",
        "index.js",
        "LICENSE",
        "src/**/*.js",
        "!pkg/*",
        "!*.spec.js"
    ],
    "homepage": "https://github.com/nestauk/svizzle",
    "keywords": [
        "file",
        "read",
        "write",
        "path",
        "functional programming",
        "utility"
    ],
    "license": "MIT",
    "main": "dist/index.js",
    "module": "index.js",
    "name": "@svizzle/file",
    "peerDependencies": {
        "lamb": "^0.58.0"
    },
    "repository": {
        "type": "git",
        "url": "github:nestauk/svizzle",
        "directory": "packages/tools/file"
    },
    "publishConfig": {
        "access": "public"
    },
    "scripts": {
        "build": "rollup -c",
        "bumppack": "npm run setprerelease && npm run pack",
        "lint": "eslint src/*.js",
        "pack": "mkdirp pkg && rimraf pkg/* && dirname=${PWD##*/} && tar -cvz -f pkg/${npm_package_version}.tar.gz -C .. ${dirname}/dist/ ${dirname}/index.js ${dirname}/LICENSE ${dirname}/package.json ${dirname}/src/",
        "setpreminor": "npm --no-git-tag-version version preminor --preid=dev",
        "setprerelease": "npm --no-git-tag-version version prerelease --preid=dev",
        "test": "mocha -r esm --recursive src"
    },
    "sideEffects": true,
    "version": "0.7.0"
}
