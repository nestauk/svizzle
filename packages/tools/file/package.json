{
	"author": "nestauk (https://www.nesta.org.uk/)",
	"bugs": {
		"url": "https://github.com/nestauk/svizzle/issues"
	},
	"config": {
		"name": "file",
		"preid": "dev"
	},
	"dependencies": {
		"@svizzle/utils": "^0.17.0",
		"d3-dsv": "^3.0.1",
		"js-yaml": "^4.1.0",
		"lamb": "^0.60.0"
	},
	"description": "Svizzle File contains file related helpers for node.",
	"devDependencies": {
		"eslint": "^8.23.0",
		"mocha": "^8.3.2",
		"nock": "^13.2.9",
		"node-fetch": "^3.1.0",
		"tempy": "^0.2.1"
	},
	"engines": {
		"node": ">=17.5.0"
	},
	"files": [
		"index.js",
		"src/*.js",
		"!*.spec.js",
		"!*.todo.js",
		"CHANGELOG.md",
		"LICENSE",
		"README.md"
	],
	"homepage": "https://github.com/nestauk/svizzle",
	"keywords": [
		"file",
		"read",
		"write",
		"path",
		"functional programming",
		"utility"
	],
	"license": "MIT",
	"main": "index.js",
	"module": "index.js",
	"name": "@svizzle/file",
	"repository": {
		"type": "git",
		"url": "github:nestauk/svizzle",
		"directory": "packages/tools/file"
	},
	"publishConfig": {
		"access": "public"
	},
	"scripts": {
		"link": "npm link",
		"lint": "eslint --max-warnings=0 src/**/*.js",
		"outdated": "npm outdated",
		"setpreminor": "npm --no-git-tag-version version preminor --preid=${npm_package_config_preid}",
		"setprepatch": "npm --no-git-tag-version version prepatch --preid=${npm_package_config_preid}",
		"setprerelease": "npm --no-git-tag-version version prerelease --preid=${npm_package_config_preid}",
		"test": "mocha --recursive src",
		"unlink": "npm unlink -g"
	},
	"sideEffects": true,
	"type": "module",
	"version": "0.13.0"
}
