{
	"author": "nestauk (https://www.nesta.org.uk/)",
	"bugs": {
		"url": "https://github.com/nestauk/svizzle/issues"
	},
	"config": {
		"name": "mapbox",
		"preid": "dev"
	},
	"dependencies": {
		"@mapbox/geo-viewport": "^0.4.1",
		"@svizzle/dom": "^0.7.3",
		"mapbox-gl": "^2.15.0"
	},
	"description": "Mapbox components and utils for Svelte.",
	"devDependencies": {
		"eslint": "^8.54.0",
		"eslint-plugin-svelte": "^2.35.0",
		"svelte": "^3.59.2"
	},
	"engines": {
		"node": ">=17.5.0"
	},
	"files": [
		"!*.spec.js",
		"src/**/*.js",
		"src/**/*.svelte",
		"CHANGELOG.md",
		"LICENSE",
		"README.md"
	],
	"homepage": "https://github.com/nestauk/svizzle",
	"keywords": [
		"mapbox",
		"chart",
		"component",
		"data visualization",
		"svelte"
	],
	"license": "MIT",
	"main": "src/index.js",
	"module": "src/index.js",
	"name": "@svizzle/mapbox",
	"publishConfig": {
		"access": "public"
	},
	"repository": {
		"directory": "packages/components/mapbox",
		"type": "git",
		"url": "github:nestauk/svizzle"
	},
	"scripts": {
		"link": "npm link",
		"lint": "eslint --max-warnings=0 'src/**/*.{js,svelte}'",
		"outdated": "npm outdated",
		"setpreminor": "npm --no-git-tag-version version preminor --preid=${npm_package_config_preid}",
		"setprepatch": "npm --no-git-tag-version version prepatch --preid=${npm_package_config_preid}",
		"setprerelease": "npm --no-git-tag-version version prerelease --preid=${npm_package_config_preid}",
		"unlink": "npm unlink -g"
	},
	"sideEffects": false,
	"svelte": "src/index.js",
	"exports": {
		".": {
			"svelte": "./src/index.js"
		}
	},
	"type": "module",
	"version": "0.2.0"
}
