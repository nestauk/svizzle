{
    "author": "nestauk (https://www.nesta.org.uk/)",
    "bugs": {
        "url": "https://github.com/nestauk/svizzle/issues"
    },
    "dependencies": {
        "@svizzle/dom": "^0.3.0",
        "@svizzle/utils": "^0.5.0",
        "just-compare": "^1.3.0",
        "lamb": "^0.58.0",
        "yootils": "^0.0.16"
    },
    "devDependencies": {
        "@svizzle/dev": "^0.3.0",
        "@svizzle/file": "^0.5.0",
        "eslint": "^6.8.0",
        "eslint-plugin-import": "^2.18.2",
        "eslint-plugin-svelte3": "^2.7.3",
        "esm": "^3.0.84",
        "jsdom": "^15.1.1",
        "jsdom-global": "^3.0.2",
        "mkdirp": "^0.5.1",
        "mocha": "^5.2.0",
        "rimraf": "^3.0.0",
        "rollup": "^1.20.0",
        "rollup-plugin-analyzer": "^3.1.2",
        "rollup-plugin-buble": "^0.19.8",
        "rollup-plugin-cleanup": "^3.1.1",
        "rollup-plugin-commonjs": "^10.0.2",
        "rollup-plugin-node-resolve": "^5.2.0",
        "rollup-plugin-svelte": "^5.1.0",
        "rollup-plugin-terser": "^5.2.0",
        "svelte": "^3.12.1"
    },
    "description": "A vertical barchart component for Svelte.",
    "files": [
        "dist/*",
        "src/BarchartV.svelte",
        "README.md",
        "LICENSE",
        "src/**/*.js",
        "src/**/*.svelte",
        "!pkg/*",
        "!*.spec.js"
    ],
    "homepage": "https://github.com/nestauk/svizzle",
    "keywords": [
        "barchart",
        "chart",
        "component",
        "data visualization",
        "svelte"
    ],
    "license": "MIT",
    "browser": "dist/BarchartV.browser.js",
    "main": "dist/BarchartV.js",
    "module": "src/BarchartV.svelte",
    "name": "@svizzle/BarchartV",
    "publishConfig": {
        "access": "public"
    },
    "repository": {
        "type": "git",
        "url": "github:nestauk/svizzle",
        "directory": "packages/components/BarchartV"
    },
    "scripts": {
        "build": "rimraf dist && rollup -c",
        "bumppack": "npm run setprerelease && npm run pack",
        "lint": "eslint src/*.{js,svelte}",
        "pack": "mkdirp pkg && rimraf pkg/* && dirname=${PWD##*/} && tar -cvz -f pkg/${npm_package_version}.tar.gz -C .. ${dirname}/dist/ ${dirname}/LICENSE ${dirname}/package.json ${dirname}/src/",
        "setpreminor": "npm --no-git-tag-version version preminor --preid=dev",
        "setprerelease": "npm --no-git-tag-version version prerelease --preid=dev",
        "test": "mocha -r esm -r jsdom-global/register --recursive src"
    },
    "sideEffects": false,
    "version": "0.1.0"
}
